{
  "name": "1click.io",
  "description": "1click ScreenSharing Extension",
  "version": "1.0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "icons": {
  	"16": "favicon.png",
  	"128": "logo.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [ {
    "js": [ "content.js" ],
    "matches": [ "https://*/*"]
  }]

}
